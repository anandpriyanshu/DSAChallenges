    "def InvertTree(root):\n",
    "\n",
    "    if root == None:\n",
    "        return root\n",
    "    \n",
    "    root.left, root.right = root.right,root.left\n",
    "\n",
    "    InvertTree(root.left)\n",
    "    InvertTree(root.right)\n",
    "\n",
    "    return root"
   ]
