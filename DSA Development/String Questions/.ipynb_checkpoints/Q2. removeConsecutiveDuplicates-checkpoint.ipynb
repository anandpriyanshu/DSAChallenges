   "source": [
    "def removeConsecutiveDuplicates(s):\n",
    "    newstr = \"\"\n",
    "    for i in range(len(s)-1):\n",
    "        if newstr!='' and newstr[len(newstr)-1]==s[i]:\n",
    "            continue\n",
    "        newstr = newstr + s[i]\n",
    "    return newstr\n",
    "            \n",
    "    \n",
    "\n",
    "s = \"daabcbaabcbc\"\n",
    "removeConsecutiveDuplicates(s)"
   ]
