    "def removeAllOccurrencesOfChar(s,ch):\n",
    "    ans = ''\n",
    "    if len(s) ==0:\n",
    "        return ''\n",
    "    for i in range(len(s)):\n",
    "        if s[i] == ch:\n",
    "            continue\n",
    "        else:\n",
    "            ans += s[i]\n",
    "    return ans\n",
    "s = 'ababaaa'\n",
    "removeAllOccurrencesOfChar(s,'a')"
   ]
