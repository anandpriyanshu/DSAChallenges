    "def ReverseEachworld(s):\n",
    "    a = s.split()\n",
    "    for i in range(len(a)):\n",
    "        a[i] = a[i][::-1]\n",
    "        \n",
    "    return (\" \").join(a)\n",
    "s = input() \n",
    "ReverseEachworld(s)"
