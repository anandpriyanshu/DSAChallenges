   "source": [
    "def largestNumber( nums):\n",
    "    s = [str(num) for num in nums]\n",
    "    s.sort(key=lambda x: x * 10, reverse=True)\n",
    "    return '0' if s[0] == '0' else ''.join(s)\n",
    "\n",
    "nums = [3,30,34,5,9]\n",
    "largestNumber( nums)"
