    "def isPalindrome(s):\n",
    "    a = \"\"\n",
    "    for x in s:\n",
    "        if x.isalpha(): \n",
    "            a += x.lower()\n",
    "        if x.isnumeric(): \n",
    "            a += x\n",
    "    return a == a[::-1]\n",
    "s = \"A man, a plan, a canal: Panama\"\n",
    "ans = isPalindrome(s)\n",
    "print(ans)"
   ]
