   "source": [
    "def ShortestPalindrome(s):\n",
    "    rev = s[::-1]\n",
    "\n",
    "    for i in range(len(s)):\n",
    "        if s.startswith(rev[i:]):\n",
    "            return rev[:i] + s\n",
    "    return rev + s\n"
   ]
  },
