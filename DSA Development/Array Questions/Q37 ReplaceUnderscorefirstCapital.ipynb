    "def rePlace(s):\n",
    "    data = s.split(\"_\")\n",
    "    print(data)\n",
    "    \n",
    "    for i in range(len(data)):\n",
    "        data[i] = data[i][0].upper() + data[i][1:]\n",
    "        print(data)\n",
    "    \n",
    "    return \" \".join(data)\n",
    "\n",
    "s = \"ui_dev_guide\"\n",
    "x = rePlace(s)\n",
    "print(x)\n"
   ]
