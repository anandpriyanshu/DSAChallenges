    "def jumpGame(a):\n",
    "\n",
    "  maxIdx = 0\n",
    "  for i in range(len(a)):\n",
    "    if i>maxIdx: return False\n",
    "    maxIdx = max(maxIdx,i+a[i])\n",
    "  return True\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "# nums = [3,2,1,0,4]\n",
    "nums = [2,3,1,1,4]\n",
    "print(jumpGame(nums))"
   ]
