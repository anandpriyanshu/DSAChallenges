    "def palindrome(s):\n",
    "    si,ei = 0, len(s)-1\n",
    "    \n",
    "    while si < ei:\n",
    "        if s[si] != s[ei]:\n",
    "            return False\n",
    "        \n",
    "        si+=1\n",
    "        ei-=1\n",
    "    return True\n",
    "s = 'kadak'\n",
    "if palindrome(s):\n",
    "    print(\"true\")\n",
    "else:\n",
    "    print(\"false\")"
   ]
