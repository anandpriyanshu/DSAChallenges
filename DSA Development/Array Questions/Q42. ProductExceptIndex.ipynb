    "def productexceptIndex(nums):\n",
    "  res = [1]*(len(nums))\n",
    "  pre =1\n",
    "  for i in range(len(nums)):\n",
    "    res[i] = pre\n",
    "    pre *= nums[i]\n",
    "  post = 1\n",
    "  for i in range(len(nums)-1,-1,-1):\n",
    "    res[i]*= post\n",
    "    post*=nums[i]\n",
    "  return res"
   ]
