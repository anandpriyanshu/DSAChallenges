    "def CountNums(arr):\n",
    "    d= {}\n",
    "    for i in arr:\n",
    "        if i  in d:\n",
    "            d[i] = d[i] + 1\n",
    "        else:\n",
    "            d[i]=1\n",
    "\n",
    "    return d\n",
    "\n",
    "\n",
    "arr = [5, 5, 5, 2, 3, 4, 2, 2, 3, 3, 4]\n",
    "print(CountNums(arr))"
   ]
  }
