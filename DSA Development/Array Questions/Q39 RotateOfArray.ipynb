    "def RotateOfArray(num,k):\n",
    "  n = len(num)\n",
    "  k = k%n\n",
    "  num.reverse()\n",
    "  num[:k] = reversed(num[k:])\n",
    "  num[k:] = reversed(num[k:])\n",
    "\n",
    "\n",
    "nums = [1,2,3,4,5,6,7]\n",
    "k = 3\n",
    "RotateOfArray(nums,k)"
   ]
  }
