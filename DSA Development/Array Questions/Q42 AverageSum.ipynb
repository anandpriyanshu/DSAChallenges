    "def AverageSumUsingSlidingWindow(a):\n",
    "\n",
    "  currSum =  maxSum = sum(a[:3])\n",
    "\n",
    "  for i in range(k,len(a)):\n",
    "    currSum += a[i] - a[i-k]\n",
    "\n",
    "    maxSum = max(maxSum,currSum)\n",
    "  \n",
    "  return currSum\n",
    "  \n",
    "\n"
   ]
