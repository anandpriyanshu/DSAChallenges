   "source": [
    "def twoSum(nums,tgt):\n",
    "    seen = {}\n",
    "    for i, val in enumerate(nums):\n",
    "\n",
    "        remaining = tgt - nums[i]\n",
    "        if remaining in seen:\n",
    "            return [i,seen[remaining]]\n",
    "        else:\n",
    "            seen[val] = i"
   ]
