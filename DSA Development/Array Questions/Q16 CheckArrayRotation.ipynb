    "def CheckArrayRotation(a):\n",
    "    count = 0\n",
    "    for i in range(len(a)-1):\n",
    "        if a[i] > a[i+1]:\n",
    "            return i + 1\n",
    "    return count\n",
    "    \n",
    "    \n",
    "    \n",
    "a = [5,6,8,7,2,3,4]\n",
    "CheckArrayRotation(a)"
