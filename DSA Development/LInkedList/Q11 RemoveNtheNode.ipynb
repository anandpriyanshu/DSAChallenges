    "def  RemoveNtheNode(head,n):\n",
    "\n",
    "    fast = head\n",
    "    slow = head\n",
    "\n",
    "    for i in range(n);\n",
    "        fast = fast.next\n",
    "\n",
    "\n",
    "    if not fast:\n",
    "        return head.next\n",
    "    \n",
    "\n",
    "    while fast.next:\n",
    "        fast = fast.next\n",
    "        slow = slow.next\n",
    "\n",
    "    slow.next = slow.next.next\n",
    "    \n",
    "    return head"
   ]
  }
