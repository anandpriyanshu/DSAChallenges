    "def Reverse(a):\n",
    "    \n",
    "    for i in range(len(a)//2):\n",
    "        a[i],a[len(a)-i-1] = a[len(a)-i-1],a[i]\n",
    "    return a\n",
    "    \n",
    "    \n",
    "    \n",
    "a = [10,20,30,40,50]\n",
    "Reverse(a)"
